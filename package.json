{
  "name": "slicknode",
  "version": "0.3.0",
  "description": "CLI tool to create GraphQL servers with slicknode",
  "bin": {
    "slicknode": "dist/app.js"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "build": "tsc",
    "clean": "rimraf dist/*",
    "watch": "tsc --watch",
    "test": "npm run lint && npm run testonly",
    "testonly": "mocha --check-leaks --exit --full-trace --require ts-node/register 'src/**/__tests__/**/*-test.{ts,tsx}'",
    "dist": "npm run clean && tsc",
    "prepublish": "npm run clean && npm run dist"
  },
  "directories": {
    "lib": "./dist"
  },
  "dependencies": {
    "adm-zip": "git://github.com/ivome/adm-zip.git",
    "babel-runtime": "^6.26.0",
    "caporal": "^0.10.0",
    "chalk": "^2.4.1",
    "es6-promisify": "^6.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "graphql": "<14.0.0",
    "inquirer": "^6.2.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.6.0",
    "js-yaml": "^3.12.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "slicknode-auth-email-password": "^0.1.2",
    "slicknode-client": "^0.2.2",
    "tar": "^4.4.6",
    "uuid": "^3.3.2",
    "validator": "^10.7.1"
  },
  "peerDependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": "slicknode/slicknode",
  "keywords": [
    "graphql",
    "slicknode"
  ],
  "author": "Ivo MeiÃŸner",
  "license": "MIT",
  "devDependencies": {
    "@types/adm-zip": "^0.4.31",
    "@types/chai": "^4.1.4",
    "@types/es6-promisify": "^6.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/graphql": "^14.0.0",
    "@types/inquirer": "^0.0.43",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/joi": "^13.4.4",
    "@types/js-yaml": "^3.11.2",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.116",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node-fetch": "^2.1.2",
    "@types/tar": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^9.4.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "mocha-sinon": "^2.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
