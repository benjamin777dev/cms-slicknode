{
  "name": "slicknode",
  "version": "0.5.0",
  "description": "GraphQL-First Application Framework and CMS for rapid software development: Serverless, declarative and extensible.",
  "bin": {
    "slicknode": "./bin/run"
  },
  "bugs": "https://github.com/slicknode/slicknode/issues",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "build": "tsc",
    "clean": "rimraf dist/*",
    "watch": "tsc --watch",
    "testonly": "mocha --check-leaks --exit --full-trace --require ts-node/register 'src/**/__tests__/**/*-test.{ts,tsx}'",
    "dist": "npm run clean && tsc && copyfiles -u 2 \"./src/templates/**/*\" ./lib/templates",
    "prepare": "npm run clean && npm run dist"
  },
  "directories": {
    "lib": "./dist"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "adm-zip": "^0.4.13",
    "babel-runtime": "^6.26.0",
    "caporal": "^0.10.0",
    "chalk": "^2.4.1",
    "cli-ux": "^5.3.0",
    "es6-promisify": "^6.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "graphql": "<14.0.0",
    "inquirer": "^6.2.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.6.0",
    "js-yaml": "^3.12.0",
    "listr": "^0.14.3",
    "listr-verbose-renderer": "^0.5.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "rimraf": "^2.6.3",
    "slicknode-auth-email-password": "^0.1.2",
    "slicknode-client": "^0.2.2",
    "tar": "^4.4.6",
    "tslib": "^1",
    "uuid": "^3.3.2",
    "validator": "^10.7.1"
  },
  "peerDependencies": {},
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/README.md",
    "/LICENSE"
  ],
  "repository": "slicknode/slicknode",
  "keywords": [
    "graphql",
    "slicknode",
    "headless",
    "serverless",
    "framework",
    "cms"
  ],
  "author": "Ivo MeiÃŸner",
  "license": "MIT",
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/adm-zip": "^0.4.31",
    "@types/chai": "^4.1.4",
    "@types/es6-promisify": "^6.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/graphql": "^14.0.0",
    "@types/inquirer": "^0.0.43",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/joi": "^13.4.4",
    "@types/js-yaml": "^3.11.2",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.116",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^10.0.3",
    "@types/node-fetch": "^2.1.2",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^7.0.13",
    "@types/tar": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^9.4.1",
    "chai": "^4.1.2",
    "copyfiles": "^2.1.0",
    "mocha": "^5.2.0",
    "mocha-sinon": "^2.1.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "slicknode",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
